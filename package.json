    "lint": "tslint --project .",
    "lint:fix": "tslint --project . --fix",
    "build": "tsc"
  },
  "dependencies": {
    "@types/express": "^4.16.0",
    "ioredis": "^3.2.2",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/ioredis": "^3.2.10",
    "@types/jest": "^23.1.3",
    "@types/uuid": "^3.4.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "jest": "^23.2.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2"
  }
}
